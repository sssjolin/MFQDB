SELECT ATTRIBUTE(S):
cust,month,_1_avg_quant,_2_avg_quant,_3_avg_quant
NUMBER OF GROUPING VARIABLES(n):
3
GROUPING ATTRIBUTES(V):
cust, month
F-VECT([F]):
_1_avg_quant, _2_avg_quant,_3_avg_quant
SELECT CONDITION-VECT([Ïƒ]):
1.cust=cust and 1.month<month and 1.year=1997
2.cust=cust and 2.month=month and 2.year=1997
3.cust=cust and 3.month>month and 3.year=1997
HAVING_CONDITION(G):
_1_avg_quant<>0 and _2_avg_quant<>0 and _3_avg_quant<>0