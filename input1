SELECT ATTRIBUTE(S):
cust,_1_avg_quant, _2_avg_quant, _3_avg_quant
NUMBER OF GROUPING VARIABLES(n):
3
GROUPING ATTRIBUTES(V):
cust
F-VECT([F]):
_1_sum_quant, _1_avg_quant, _2_avg_quant, _3_sum_quant, _3_avg_quant
SELECT CONDITION-VECT([Ïƒ]):
1.cust=cust and 1.state='NY' and 1.year=1997
2.cust=cust and 2.state='CT' and 2.year=1997
3.cust=cust and 3.state='NJ' and 3.year=1997
HAVING_CONDITION(G):
_1_avg_quant > _2_avg_quant and _1_avg_quant > _3_avg_quant